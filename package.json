{
    "name": "odata-modeller",
    "version": "0.0.1",
    "description": "A tool for creating graphical representations of OData services.",
    "main": "index.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/bradzacher/odata-modeller.git"
    },
    "keywords": [
        "odata",
        "model",
        "service"
    ],
    "author": "Brad Zacher",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/bradzacher/odata-modeller/issues"
    },
    "homepage": "https://github.com/bradzacher/odata-modeller#readme",
    "scripts": {
        "build": "webpack --config webpack.production.config.js --progress --profile --colors",
        "start": "webpack-dev-server --progress --profile --colors",
        "lint": "eslint --ext js --ext jsx src || exit 0"
    },
    "dependencies": {
        "babel-core": "6.18.2",
        "babel-loader": "6.2.7",
        "babel-plugin-transform-class-properties": "^6.19.0",
        "babel-plugin-transform-decorators-legacy": "^1.3.4",
        "babel-plugin-transform-runtime": "^6.15.0",
        "babel-preset-es2015": "6.18.0",
        "babel-preset-react": "6.16.0",
        "babel-runtime": "^6.18.0",
        "css-loader": "0.26.0",
        "eslint": "^3.11.1",
        "eslint-config-airbnb-base": "^10.0.1",
        "eslint-plugin-import": "^2.2.0",
        "eslint-plugin-react": "^6.7.1",
        "extend": "^3.0.0",
        "extract-text-webpack-plugin": "^1.0.1",
        "file-loader": "0.9.0",
        "html-webpack-plugin": "^2.24.1",
        "immutable": "^3.8.1",
        "node-sass": "^3.13.0",
        "postcss-loader": "^1.1.1",
        "react": "15.4.1",
        "react-dom": "15.4.1",
        "react-hot-loader": "^3.0.0-beta.6",
        "react-mdl": "^1.9.0",
        "redux": "^3.6.0",
        "redux-thunk": "^2.1.0",
        "sass-loader": "^4.0.2",
        "style-loader": "0.13.1",
        "url-loader": "0.5.7",
        "webpack": "1.13.3",
        "webpack-cleanup-plugin": "^0.4.1",
        "webpack-dev-server": "1.16.2"
    },
    "eslintConfig": {
        "globals": {},
        "env": {
            "browser": true,
            "commonjs": true,
            "es6": true
        },
        "extends": [
            "airbnb-base"
        ],
        "parserOptions": {
            "ecmaVersion": 6,
            "ecmaFeatures": {
                "experimentalObjectRestSpread": true,
                "jsx": true
            },
            "sourceType": "module"
        },
        "plugins": [
            "react"
        ],
        "rules": {
            "indent": [
                "error",
                4,
                {
                    "SwitchCase": 1
                }
            ],
            "linebreak-style": [
                "error",
                "windows"
            ],
            "quotes": [
                "error",
                "single"
            ],
            "semi": [
                "error",
                "always"
            ],
            "spaced-comment": [
                "warn",
                "always",
                {
                    "exceptions": [
                        "*"
                    ]
                }
            ],
            "max-len": [
                "warn",
                200
            ],
            "no-multi-spaces": [
                "warn",
                {
                    "exceptions": {
                        "VariableDeclarator": true
                    }
                }
            ],
            "no-unused-expressions": [
                "error",
                {
                    "allowShortCircuit": true
                }
            ],
            "strict": [
                "off"
            ],
            "no-param-reassign": [
                "warn",
                {
                    "props": false
                }
            ],
            "class-methods-use-this": 0,
            "react/jsx-uses-vars": [
                2
            ]
        }
    },
    "babel": {
        "presets": [
            "es2015",
            "react"
        ],
        "plugins": [
            "transform-runtime",
            "transform-decorators-legacy",
            "transform-class-properties",
            "react-hot-loader/babel"
        ]
    }
}
